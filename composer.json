{
    "name" 			: "knowthecode/fulcrum-site",
    "description" 	: "Site Specific Plugin - This plugin is an addon for Fulcrum",
    "type"          : "project",
    "keywords" 		: ["wordpress", "wp", "plugin"],
    "homepage" 		: "https://knowthecode.io",
    "license" 		: "GPL-2.0+",
    "authors": [
        {
            "name" 	: "hellofromTonya",
            "email" : "hellofromtonya@knowthecode.io"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {},
    "autoload" : {
        "classmap": [
            "src/class-plugin.php",

            "src/shortcode/class-info-box.php",

            "src/widget/class-member-nav-widget.php"
        ],
        "files": [
            "src/admin/metaboxes/header.php",
            "src/structure/grid.php",
            "src/structure/post.php",
            "src/foundation/emojis-support.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "vendor-dir": "assets/vendor"
    },
    "minimum-stability": "dev"
}